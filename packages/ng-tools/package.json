  "dependencies": {
    "@types/cheerio": "^0.22.30",
    "chalk": "^4.1.2",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "tsyringe": "^4.6.0",
    "yarn": "^1.22.11"
  },
  "devDependencies": {
    "@ngx-composer/tsconfig": "workspace:*",
