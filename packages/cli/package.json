{
    "name": "@ngx-composer/cli",
    "version": "1.0.4",
    "description": "Composes multiple Angular workspaces for building, serving and more without an overhead of configuration.",
    "main": "dist/index.js",
    "bin": {
        "ngx": "./dist/index.js"
    },
    "private": false,
    "repository": {
        "url": "https://github.com/hoevelmanns/ngx-composer.git"
    },
    "scripts": {
        "build": "ncc build src/index.ts -o ./dist -e fs-extra",
        "watch": "ncc build src/index.ts -w -o ./dist -e fs-extra",
        "prepare": "pnpm run build"
    },
    "author": "Marcelle HÃ¶velmanns <mail@hoevelmanns.io>",
    "license": "ISC",
    "devDependencies": {
        "@ngx-composer/tsconfig": "workspace:*",
        "@types/fs-extra": "^9.0.12",
        "@types/is-glob": "^4.0.2",
        "@types/node": "^16.4.7",
        "@types/yargs": "^17.0.2",
        "typescript": "^4.3.5",
        "@vercel/ncc": "^0.24.1"
    },
    "dependencies": {
        "@ngx-composer/ng-tools": "workspace:*",
        "chalk": "^4.1.2",
        "eta": "^1.12.3",
        "execa": "^5.1.1",
        "fast-glob": "^3.2.7",
        "fs-extra": "^10.0.0",
        "is-glob": "^4.0.1",
        "listr2": "^3.11.0",
        "reflect-metadata": "^0.1.13",
        "strip-json-comments": "^3.1.1",
        "table": "^6.7.1",
        "tsyringe": "^4.6.0",
        "yargs": "^17.1.0"
    }
}
